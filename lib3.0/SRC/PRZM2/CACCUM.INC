C**** THE FOLLOWING ADDED FOR LATERAL PESTICIDE FLUXES ********
     +              MOUTP9,YOUTP9,MLOUT,YLOUT
C
C     DINFLO, MINFLO, and YINFLO are accumulators for lateral inflow of
C     water from septic effluent.  DSNINF, MSNINF, and YSNINF are
C     accumulators for inflow of nitrogen species from septic effluent.
C
      COMMON /ACCUMR4/ MOUTW5,YOUTW5,MINPW1,MINPW2,MOUTW1,MOUTW2,MOUTW3, 
     1                 MOUTW4,MSTR1,MSTR2,YINPW1,YINPW2,YOUTW1,YOUTW2,
     2                 YOUTW3,YOUTW4,YSTR1,YSTR2,YOUTW6,MOUTW6,DOUTFL,
     3                 MOUTFL,YOUTFL,MOUTPV,YOUTPV,DINFLO,MINFLO,YINFLO,
     4                 DSNINF,MSNINF,YSNINF
C
      COMMON /ACCUMR41/ MINPW(NCMPTS),MOUTW(NCMPTS),MEOUTW(NCMPTS),
     1                  YINPW(NCMPTS),YOUTW(NCMPTS),YEOUTW(NCMPTS),
     2                  MSTR(NCMPTS),YSTR(NCMPTS),MOUTP5(3),MOUTP6(3),
     3                  YOUTP2(3),MINPP1(3),MOUTP1(3),MOUTP2(3),
     4                  MOUTP3(3),MOUTP4(3),MSTRP1(3),YINPP1(3),
     5                  YOUTP1(3),YOUTP3(3),YOUTP4(3),YSTRP1(3),
     6                  YOUTP5(3),YOUTP6(3),DCOFLX(3),MCOFLX(3),
     7                  YCOFLX(3),MOOUTW(NCMPTS),YOOUTW(NCMPTS),
     8                  MOUTP7(3),YOUTP7(3),MOUTP8(3),YOUTP8(3),
     9                  MINPP8(3),YINPP8(3),
C**** THE FOLLOWING ADDED FOR LATERAL PESTICIDE FLUXES ********
     +                  MOUTP9(3),YOUTP9(3)
C
      COMMON /ACCUMR42/ MOUTP(3,NCMPTS),YDOUT(3,NCMPTS),MINPP(3,NCMPTS),
     1                  MDOUT(3,NCMPTS),YINPP(3,NCMPTS),YOUTP(3,NCMPTS),
     2                  MSTRP(3,NCMPTS),YSTRP(3,NCMPTS),
     3                  MINPP2(3,NCMPTS),YINPP2(3,NCMPTS),
     4                  VOUTM(3,NCMPTS),VOUTY(3,NCMPTS),MTRFM(3,NCMPTS),
     5                  YTRFM(3,NCMPTS),
