C
C     include file CHYDR.INC
C
         REAL       SW,WP,FC,ET,CINT,CEVAP,AFIELD,USLEK,USLELS,USLEC,
     1              USLEP,DELX,THETN,INABS,SMELT,BD,THETO,CINTB,DIN,
     2              AINF,RUNOF,THETAS,AD,VEL,THEFC,THEWP,WBAL,CWBAL,
     3              OSNOW,CORED,THETH,SEDL,TDET,SAND,CLAY,SNOW,
     4              ADL,OUTFLO,DPN,Z,ZC,DEN,TOP
         INTEGER    NCOM1,NCOM2,CN,NCOM0,NCOM2M,ICNAH,HSWZT,ISCOND,
     1              ERFLAG,NUM,VLFLAG,NPI,IRFLAG,MCOFLG,RATIO,
     2              PCOUNT,ICROSS,STRYR,ENDYR,MOCFLG,FRMFLG
C
      COMMON /HYRI/ NCOM1,NCOM2,HSWZT,ISCOND,NCOM0,FRMFLG,NCOM2M,
     1               ERFLAG,STRYR,ENDYR,IRFLAG,NUM,VLFLAG,MCOFLG,
     2               MOCFLG
C
      COMMON /HYRI1/ ICNAH(NC),NPI(3),ICROSS(3)
C
      COMMON /HYRI2/ RATIO(3,NCMPTS),PCOUNT(3,NCMPTS)
C
      COMMON /HYRI3/ CN(NC,3,3)
C               
      COMMON /HYRR4/ THETH,CINT,CEVAP,AFIELD,INABS,SEDL,TDET,CORED,
     1                SMELT,CINTB,DIN,RUNOF,WBAL,CWBAL,SNOW,OSNOW,
     2                USLEK,USLELS,USLEP
C
      COMMON /HYRR41/ SW(NCMPTS),WP(NCMPTS),FC(NCMPTS),ET(NCMPTS),
     1                 CLAY(NCMPTS),THETN(NCMPTS),BD(NCMPTS),
     2                 THETO(NCMPTS),AINF(NCMPTS),THETAS(NCMPTS),
     3                 AD(NCMPTS),SAND(NCMPTS),VEL(NCMPTS),
     4                 THEFC(NCMPTS),THEWP(NCMPTS),ADL(NCMPTS),
     5                 OUTFLO(NCMPTS),DELX(NCMPTS),DPN(NCMPTS),
     6                 ZC(NCMPTS)
C
      COMMON /HYRR42/ USLEC(NC,3),Z(3,NPII),DEN(3,NCMPTS),
     1                 TOP(3,NCMPTS-1)
C
