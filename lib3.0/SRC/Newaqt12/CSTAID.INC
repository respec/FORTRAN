      CHARACTER*40 STANAM(MXSTA)
      CHARACTER*8  LOCID(MXSTA)
      CHARACTER*16 XCONNM(MXCONX)
      CHARACTER*10 XSENNM(MXSENX)
C
      COMMON /STAID/XNSEN,XNCON,XNDSN,XTSCON,XCONID,XTSSEN,
     $              XTSLOC,XTSDSN
C
      INTEGER      XNSEN,XNCON,XNDSN,XTSCON(MXDSNX),XCONID(MXCONX),
     $             XTSSEN(MXDSNX),XTSLOC(MXDSNX),XTSDSN(MXDSNX)
