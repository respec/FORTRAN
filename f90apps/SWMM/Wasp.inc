C  EXTRAN Common blocks for the linking of EXTRAN to WASP
C  CIM  5/99
C     MAXWASP IS MAXIMUM NUMBER OF WASP SEGMENTS
C     MAXJUN IS THE MAXIMUM NUMBER OF EXTRAN JUNCTIONS PER
C     WASP SEGMENT
C     MAXCON IS THE MAXIMUM NUMBER OF EXTRAN CONDUITS THAT CAN
C     REPRESENT FLOWS BETWEEN TWO WASP SEGMENTS
      PARAMETER (MAXWASP=350,MAXJUN=5,MAXCON=5)
      COMMON /WASP1/ WTSTART,IDEP,NSTEPW,NWASP,NINTER,IWASP,WASPTIME,
     1               IWASPREC,LVCALC
      LOGICAL LVCALC
      COMMON /WASP2/ NJUNSEG(MAXWASP),ICONVEL(MAXWASP),ACONVEL(MAXWASP),
     1   JUNSEG(MAXJUN,MAXWASP)
      COMMON /WASP3/ FROMSEG(MAXWASP),TOSEG(MAXWASP),NCONSEG(MAXWASP),
     1   ICONSEG(MAXCON,MAXWASP)
      INTEGER FROMSEG,TOSEG
      COMMON /WASP4/ AJUNSEG(MAXJUN,MAXWASP),ACONSEG(MAXCON,MAXWASP)
      CHARACTER*10 AJUNSEG,ACONSEG,ACONVEL
      COMMON /WASP5/ WASPFLOW(MAXWASP),WASPVEL(MAXWASP)
      COMMON /WASP6/ WVOL(MAXWASP),WVEL(MAXWASP),WDEP(MAXWASP)
