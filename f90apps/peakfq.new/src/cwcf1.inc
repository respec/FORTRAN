C     common for Bulletin-17 computations
      COMMON/ WCFCM1 / GENSKU,IGSOPT,RMSEGS,GAGEB,QLWOUT,QHIOUT,NHIST,
     $                 HISTPD, BEGYR, ENDYR, WRCBAS,WRCPAB,WRCUAV,
     $                 WRCUSD,WRCSKW,WRCAAV,WRCASD,WRCASK,WRCHHB,WRCHOT,
     $                 WRCLOW,NMISS,NSYS,NBGB,NLWOUT,NHIOUT,NHISTN,
     $                 HISTPN,HISTWT,GSKWGT,SYSBAS,SYSPAB,SYSUAV,
     $                 SYSUSD,SYSSKW,SYSAAV,SYSASD,SYSASK,
     $                 SYSMIN,SYSMAX,SUMS,SUMH,WORK,LOTYPE,ASMSEG
      INTEGER  IGSOPT, NBGB, NHIOUT, NHIST, NHISTN, NLWOUT, NMISS, NSYS,
     $         BEGYR, ENDYR
      REAL     GAGEB, GENSKU, GSKWGT, HISTPD, HISTPN, HISTWT, RMSEGS,
     #         QHIOUT, QLWOUT, SYSAAV, SYSASD, SYSASK, SYSBAS, SYSMAX,
     #         SYSMIN, SYSPAB, SYSSKW, SYSUAV, SYSUSD, WORK(MXINT), 
     #         WRCAAV, WRCASD, WRCASK, WRCBAS, WRCHHB, WRCHOT, WRCLOW, 
     #         WRCPAB, WRCSKW, WRCUAV, WRCUSD, ASMSEG
      DOUBLEPRECISION SUMS(3), SUMH(3)
      CHARACTER*4 LOTYPE
C
C     GENSKU = GENERALIZED SKEW
C     IGSOPT = GEN SKEW OPTION = 0=WEIGHTED, 1=GEN SKEW, -1=STATION SKEW
C     RMSEGS = RMS ERROR OF GEN SKEW (0. YIELDS DEFAULTVALUE.)
C     ASMSEG = AT-SITE MSE OF SKEW
C     GAGEB  = GAGE BASE DISCHARGE                                     - NON-LOG
C     QLWOUT,QHIOUT = USER-SUPPLIED HI AND LOW OUTLIER (USE 0 IF NONE) - NON-LOG
C     NHIST  = NUMBER OF HISTORIC PEAKS
C     NSYS   = NUMBEROF SYSTEMATIC PEAKS (SEVERAL ITEMS FURTHER DOIWN IN
C              THE COMMON LIST....)
C
C     THE ABOVE VALUES ARE NEEDED AS INPUT BY CALLING PGM.
C     THE OTHERS ARE USED INTERNALLY OR RETURNED AS OUTPUT.  THE SIGNIFICANT
C     OUTPUT RESULTS ARE ALREADY PRINTED BY J407 AND  CALLING PGM DOESNT
C     NEED TO WORRY ABOUT PRINTING THEM.
C
C HISTPD - HISTORIC PERIOD (TOTAL) SPECIFIED
C BEGYR  - BEGINNING YEAR OF ANALYSIS
C ENDYR  - ENDING YEAR OF ANALYSIS
C WRCBAS - WRC FREQ CURVE FLOOD BASE LOG
C WRCPAB - WRC FREQ CURVE PROB OF FLOOD ABOVE BASE
C WRCUAV - WRC FREQ CURVE UNCONDITIONAL AVERAGE LOG
C WRCUSD - WRC FREQ CURVE UNCONDITIONAL STD DEVN LOG
C WRCSKW - WRC FREQ CURVE FINAL WRC WEIGHTED SKEW LOG
C WRCAAV - WRC FREQ CURVE ABOVE-BASE AVERAGE LOG
C WRCASD - WRC FREQ CURVE ABOVE-BASE STD DEVN LOG
C WRCASK - WRC FREQ CURVE ABOVE-BASE (STATION) SKEW LO&S
C WRCHHB - WRC FREQ CURVE HIGH-OUTLIER-HISTORIC BASE LOG
C WRCHOT - WRC HIGH OUTLIER CRITERION  LOG
C WRCLOW - WRC LOW OUTLIER CRITERION LOG
C NMISS  - NUMBER PEAKS CODED MISSING-DISCHARGE
C NSYS   - NUMBER SYSTEMATIC PEAKS
C NBGB   - NUMBER SYSTEMATIC PEAKS BELOW GAGE BASE
C NLWOUT - NUMBER SYSTEMATIC PEAKS BELOW LOW-OUT CRIT ABOVE GAGEBASE
C NHIOUT - NUMBER SYSTEMATIC PEAKS ABOVE HIGH-OUTLIER CRIT
C NHISTN - NUMBER HISTORIC PEAKS NOTED
C HISTPN - HISTORIC PERIOD (TOTAL) NOTED
C HISTWT - HISTORIC WEIGHT
C GSKWGT - GEN SKEW WEIGHT
C SYSBAS - SYS REC FREQ CURVE FLOOD BASE LOG
C SYSPAB - SYS REC FREQ CURVE PROB OF FLOOD ABOVE BASE
C SYSUAV - SYS REC FREQ CURVE UNCONDITIONAL AVERAGE LOG
C SYSUSD - SYS REC FREQ CURVE UNCONDITIONAL STD DEVN LOG
C SYSSKW - SYS REC FREQ CURVE SKEW COEFF LOG
C SYSAAV - SYS REC FREQ CURVE ABOVE-BASE AVERAGE LOG
C SYSASD - SYS REC FREQ CURVE ABOVE-BASE STD DEVN LOG
C SYSASK - SYS REC FREQ CURVE ABOVE-BASE SKEW  LOG
C SYSMIN - SYS REC MINIMUM ABOVE-GAGE-BASE LOG
C SYSMAX - SYS REC MAXIMUM LOG
C SUMS   - SUM OF POWERS OF LOGS NON-HIST-NON-OUTLIER
C SUMS   - DOUBLEPRECISE DIMENSION 3
C SUMH   - SUM OF POWERS OF LOGS HIST-HIGH-OUTLIER.
C SUMH   - DOUBLEPRECISE DIMENSION 3
C WORK   - DIM(31)- FREQ CURVE ORDINATES WORK VECTOR WCFFCA-FCX
C LOTYPE - LOW OUTLIER TEST TYPE (NONE, GBT, MGBT, FIXE)